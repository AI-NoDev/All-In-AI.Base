{
  "name": "@qiyu-allinai/i18n",
  "version": "0.1.0",
  "type": "module",
  "main": "./paraglide/messages.js",
  "module": "./paraglide/messages.js",
  "types": "./paraglide/messages.d.ts",
  "exports": {
    ".": {
      "types": "./paraglide/messages.d.ts",
      "import": "./paraglide/messages.js",
      "require": "./paraglide/messages.js",
      "default": "./paraglide/messages.js"
    },
    "./runtime": {
      "types": "./paraglide/runtime.d.ts",
      "import": "./paraglide/runtime.js",
      "require": "./paraglide/runtime.js",
      "default": "./paraglide/runtime.js"
    },
    "./messages": {
      "types": "./paraglide/messages.d.ts",
      "import": "./paraglide/messages.js",
      "require": "./paraglide/messages.js",
      "default": "./paraglide/messages.js"
    },
    "./messages/zh-Hans.json": {
      "import": "./messages/zh-Hans.json",
      "require": "./messages/zh-Hans.json",
      "default": "./messages/zh-Hans.json"
    },
    "./messages/en.json": {
      "import": "./messages/en.json",
      "require": "./messages/en.json",
      "default": "./messages/en.json"
    },
    "./server": {
      "import": "./paraglide/server.js",
      "require": "./paraglide/server.js",
      "default": "./paraglide/server.js"
    }
  },
  "files": [
    "paraglide/",
    "messages/"
  ],
  "scripts": {
    "build": "paraglide-js compile --project ./project.inlang --outdir ./paraglide && bun run generate-types",
    "generate-types": "bun scripts/generate-types.ts",
    "machine-translate": "inlang machine translate --project project.inlang"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@inlang/paraglide-js": "^2.10.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
