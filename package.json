{
  "name": "qiyu-allinai",
  "version": "1.0.0",
  "description": "All-In-AI 企业级 AI 驱动管理系统 - 基于 Turborepo 的 Monorepo 架构，集成 AI 能力、即时通讯、知识库管理等功能",
  "author": "AI-NoDev",
  "license": "MIT",
  "homepage": "https://github.com/AI-NoDev/All-In-AI.Base",
  "repository": {
    "type": "git",
    "url": "https://github.com/AI-NoDev/All-In-AI.Base.git"
  },
  "bugs": {
    "url": "https://github.com/AI-NoDev/All-In-AI.Base/issues"
  },
  "keywords": [
    "ai",
    "enterprise",
    "management-system",
    "turborepo",
    "monorepo",
    "svelte",
    "sveltekit",
    "elysia",
    "bun",
    "drizzle-orm",
    "postgresql",
    "redis",
    "minio",
    "websocket",
    "i18n",
    "rbac"
  ],
  "module": "index.ts",
  "type": "module",
  "private": true,
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "db:generate": "bun run --filter=@qiyu-allinai/db generate",
    "db:migrate": "bun run --filter=@qiyu-allinai/db migrate",
    "db:push": "bun run --filter=@qiyu-allinai/db push",
    "db:seed": "bun run --filter=@qiyu-allinai/db seed",
    "db:reset": "bun run --filter=@qiyu-allinai/db resetSeedData",
    "db:init": "bun run db:push && bun run db:reset",
    "prod:db:push": "dotenv -e .env.production -- bun run db:push",
    "prod:db:init": "dotenv -e .env.production -- bun run db:init",
    "dev:frontend": "bun run --filter=@qiyu-allinai/frontend dev",
    "dev:server": "bun run --filter=@qiyu-allinai/server dev",
    "dev:docs": "bun run --filter=@qiyu-allinai/docs dev",
    "build:frontend": "bun run --filter=@qiyu-allinai/frontend build",
    "build:server": "bun run --filter=@qiyu-allinai/server build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "dotenv-cli": "^8.0.0",
    "svelte": "catalog:",
    "turbo": "^2.7.5",
    "vite": "^7.3.1",
    "vue": "^3.5.27",
    "vuepress": "^2.0.0-rc.26"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "overrides": {
    "svelte": "$svelte",
    "svelte-lexical": "0.6.4",
    "lexical": "0.40.0",
    "@lexical/clipboard": "0.40.0",
    "@lexical/code": "0.40.0",
    "@lexical/dragon": "0.40.0",
    "@lexical/extension": "0.40.0",
    "@lexical/file": "0.40.0",
    "@lexical/hashtag": "0.40.0",
    "@lexical/history": "0.40.0",
    "@lexical/html": "0.40.0",
    "@lexical/link": "0.40.0",
    "@lexical/list": "0.40.0",
    "@lexical/mark": "0.40.0",
    "@lexical/markdown": "0.40.0",
    "@lexical/offset": "0.40.0",
    "@lexical/overflow": "0.40.0",
    "@lexical/plain-text": "0.40.0",
    "@lexical/rich-text": "0.40.0",
    "@lexical/selection": "0.40.0",
    "@lexical/table": "0.40.0",
    "@lexical/text": "0.40.0",
    "@lexical/utils": "0.40.0",
    "@lexical/yjs": "0.40.0"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "svelte": "^5.51.1",
      "zod": "^4.3.6",
      "@sveltejs/adapter-static": "^3.0.10",
      "@sveltejs/kit": "^2.52.0",
      "@sveltejs/vite-plugin-svelte": "^6.2.4",
      "@tailwindcss/vite": "^4.1.18",
      "tailwind-merge": "^3.4.0",
      "tailwind-variants": "^3.2.2",
      "tailwindcss": "^4.1.18",
      "tw-animate-css": "^1.4.0",
      "typescript": "^5.9.3",
      "typescript-eslint": "^8.54.0",
      "svelte-check": "^4.4.0",
      "svelte-sonner": "^1.0.7",
      "elysia": "^1.4.0",
      "@sinclair/typebox": "^0.34.33"
    }
  },
  "packageManager": "bun@1.3.6",
  "dependencies": {
    "svelte-markdown": "^0.4.1"
  }
}
