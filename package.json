{
  "name": "qiyu-allinai",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "db:generate": "bun run --filter=@qiyu-allinai/db generate",
    "db:migrate": "bun run --filter=@qiyu-allinai/db migrate",
    "db:push": "bun run --filter=@qiyu-allinai/db push",
    "db:seed": "bun run --filter=@qiyu-allinai/db seed",
    "db:reset": "bun run --filter=@qiyu-allinai/db resetSeedData",
    "db:init": "bun run db:push && bun run db:reset",
    "prod:db:push": "dotenv -e .env.production -- bun run db:push",
    "prod:db:init": "dotenv -e .env.production -- bun run db:init"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "dotenv-cli": "^8.0.0",
    "svelte": "catalog:",
    "turbo": "^2.7.5",
    "vite": "^7.3.1",
    "vue": "^3.5.27",
    "vuepress": "^2.0.0-rc.26"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "overrides": {
    "svelte": "$svelte"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "svelte": "^5.51.1",
      "zod": "^4.3.6",
      "@sveltejs/adapter-static": "^3.0.10",
      "@sveltejs/kit": "^2.52.0",
      "@sveltejs/vite-plugin-svelte": "^6.2.4",
      "@tailwindcss/vite": "^4.1.18",
      "tailwind-merge": "^3.4.0",
      "tailwind-variants": "^3.2.2",
      "tailwindcss": "^4.1.18",
      "tw-animate-css": "^1.4.0",
      "typescript": "^5.9.3",
      "typescript-eslint": "^8.54.0",
      "svelte-check": "^4.4.0",
      "svelte-sonner": "^1.0.7",
      "elysia": "^1.4.0",
      "@sinclair/typebox": "^0.34.33"
    }
  },
  "packageManager": "bun@1.3.6"
}
